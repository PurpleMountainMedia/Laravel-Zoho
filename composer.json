{
    "name": "purplemountain/laravel-zoho",
    "description": "Laravel wrapper for the Zoho PHP SDK",
    "authors": [
        {
            "name": "Christian Braybrooke",
            "email": "chris@purplemountmedia.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PurpleMountain\\LaravelZoho\\": "src/"
        }
    },
    "require": {
        "zohocrm/php-sdk-archive": "^2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "PurpleMountain\\LaravelZoho\\ServiceProvider"
            ],
            "aliases": {
                "LaravelZoho": "PurpleMountain\\LaravelZoho\\Facade"
            }
        }
    }
}
